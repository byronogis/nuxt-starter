{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "cz",
    "postinstall": "nuxt prepare && npx simple-git-hooks"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-nightly@latest",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.2",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/types": "^19.0.3",
    "@nuxt/eslint": "^0.3.4",
    "@unocss/eslint-config": "^0.59.0",
    "@unocss/nuxt": "^0.59.0",
    "@unocss/reset": "^0.59.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "commitizen": "^4.3.0",
    "eslint": "^9.0.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.38.1",
    "nuxt": "^3.11.2",
    "postcss-nested": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.4",
    "unocss": "^0.59.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint -e",
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
